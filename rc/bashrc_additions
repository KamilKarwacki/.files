alias vim=nvim
alias vimdiff="nvim -d"
alias sl=ls

# git related stuff
alias gits="git status"

export PATH=$PATH:${HOME}/.config/coderv2/dotfiles/scripts

if [ -f $HOME/.config/coderv2/dotfiles/rc/bashrc_functions ]; then
    source $HOME/.config/coderv2/dotfiles/rc/bashrc_functions
fi

set -o vi
